  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "bugs": {
    "url": "https://github.com/chelsea-codes/friend-chat-app/issues"
  },
  "homepage": "https://github.com/chelsea-codes/friend-chat-app#readme",
  "keywords": [
    "simple",
    "friends",
    "friend",
    "chat",
    "app",
    "application"
  ],
  "license": "GPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chelsea-codes/friend-chat-app.git"
  }
